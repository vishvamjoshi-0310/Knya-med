                           {%- if  section.settings.backgroundImage2.width >= 4096 -%}{{  section.settings.backgroundImage2 | img_url: '4096x' }} 4096w,{%- endif -%}
                           {{ section.settings.backgroundImage2 | img_url: 'master' }} {{ section.settings.backgroundImage2.width }}w"
                   src="{{ section.settings.backgroundImage2 | img_url: '' }}"
                   sizes="100vw"
                   alt="{{ section.settings.backgroundImage2.alt | escape }}"
                   loading="lazy" class="mobileimage"
                   width="{{ section.settings.backgroundImage2.width }}"
                   height="{{ section.settings.backgroundImage2.height }}"    
                   >	
              </div>
    {% elsif request.path contains 'tagged' %}
<div class="blog-banner-imgWrapper">
                    <img srcset="{%- if  section.settings.backgroundImagetwo1.width >= 550 -%}{{ section.settings.backgroundImagetwo1 | img_url: '550x' }} 550w,{%- endif -%}
                           {%- if  section.settings.backgroundImagetwo1.width >= 1100 -%}{{ section.settings.backgroundImagetwo1 | img_url: '1100x' }} 1100w,{%- endif -%}
