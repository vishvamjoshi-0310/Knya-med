         <div class="blog-banner-text-over-image">
           <div class="blog-banner-textSubtitle">           
 {% if request.path contains 'tagged' %}
     <h1 class="blog-banner-subtitle tagged">{{current_tags }}  </h1>
   {% else %}
                <h2 class="blog-banner-subtitle">{{section.settings.subtitle}}  </h2>
          {% endif %}
           </div>
          
         </div>
         
    </div>
  </div>
</div>
  {% endif %}