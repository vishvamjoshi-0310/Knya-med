            <img srcset="{%- if  section.settings.backgroundImagetwo2.width >= 550 -%}{{ section.settings.backgroundImagetwo2 | img_url: '550x' }} 550w,{%- endif -%}
                           {%- if  section.settings.backgroundImagetwo2.width >= 1100 -%}{{ section.settings.backgroundImagetwo2 | img_url: '1100x' }} 1100w,{%- endif -%}
