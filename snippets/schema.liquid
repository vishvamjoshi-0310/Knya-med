<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Organization",
  "name": {{ shop.name | json }},
  "url": {{ request.origin | json }},
  "logo": "https://cdn.shopify.com/s/files/1/0562/9247/5063/files/knya_logo_500x150_10e3d37e-4db1-4821-8a43-cd9d80c87dce_300x.png?v=1685012514",
  "sameAs": [
    "https://www.instagram.com/knyamed/",
    "https://www.linkedin.com/company/knya-med/",
    "https://www.facebook.com/knyamed/"
   
  ]
}
</script>

{% if template.name == "product" %}
{% assign judgme_review = product.metafields.judgeme.badge %}
  {% assign judgme_widget_review = product.metafields.judgeme.widget %}
{% assign average_rating = judgme_review | split: "data-average-rating='" | last | split: "'" | first %}
{% assign number_of_reviews = judgme_review | split: "data-number-of-reviews='" | last | split: "'" | first %}
{% assign first_variant = product.selected_or_first_available_variant %}
  
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Product",
  "name": "{{ product.title | escape }}",
  "image": [
    "{{ product.featured_image | img_url: 'master' | absolute_url }}"
  ],
  "description": "{{ product.description | strip_html | escape }}",
  "sku": "{{ first_variant.sku }}",
  "brand": {
    "@type": "Brand",
    "name": "Knya"
  },
  "offers": {
    "@type": "Offer",
    "priceCurrency": "{{ shop.currency }}",
    "price": "{{ product.price | money_without_currency | replace: ',', '' }}",
    "availability": "https://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}",
    "url": "{{ shop.url }}{{ product.url }}"
  }
 {% if product.metafields.judgeme.badge %}
  ,
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "{{ average_rating }}",
    "reviewCount": "{{ number_of_reviews }}"
  }
{% endif %}

}
</script>

{% if product.metafields.judgeme.badge %}
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "Review",
      "author": {
        "@type": "Person",
        "name": "{{ reviewer_name }}"
      },
      "reviewRating": {
        "@type": "Rating",
        "ratingValue": "{{ average_rating }}",
        "bestRating": "{{ number_of_reviews }}"
      },
      "reviewBody": "{{ reviewBody }}"
    }
    </script>
{% endif %}

{% endif %}

{% if template.name == "collection" %}

<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "ItemList",
  "name": "{{ collection.title }}",
  "description": "{{ collection.description | strip_html | escape }}",
  "url": "{{ shop.url }}{{ collection.url }}",
  "itemListElement": [
    {% for product in collection.products %}
    {
      "@type": "Product",
      "name": "{{ product.title | escape }}",
      "image": "{{ product.featured_image | img_url: 'master' | absolute_url }}",
      "url": "{{ shop.url }}{{ product.url }}",
      "offers": {
        "@type": "Offer",
        "priceCurrency": "{{ shop.currency }}",
        "price": "{{ product.price | money_without_currency | replace: ',', '' }}",
        "availability": "https://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}"
      }
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
</script>


{% endif %}



{% if template.name == "article" %}
          <script type="application/ld+json">
          {
          "@context": "https://schema.org",
          "@type": "BlogPosting",
          "headline": {{ article.title | json }},
          "image": {{ article.image | img_url: 'master' | absolute_url | json }},
          "author": {
          "@type": "Person",
          "name": {{ article.author | json }}
          },
          "publisher": {
          "@type": "Organization",
          "name": {{ shop.name | json }},
          "logo": {
          "@type": "ImageObject",
          "url": "https://cdn.shopify.com/s/files/1/0562/9247/5063/files/knya_logo_500x150_10e3d37e-4db1-4821-8a43-cd9d80c87dce_300x.png?v=1685012514"
          }
          },
          "datePublished": "{{ article.published_at | date: '%Y-%m-%d' }}",
          "dateModified": "{{ article.updated_at | date: '%Y-%m-%d' }}",
          "mainEntityOfPage": {
          "@type": "WebPage",
          "@id": "{{ shop.url }}{{ article.url }}"
          }
          {% if article.excerpt != blank %},
          "description": {{ article.excerpt | strip_html | json }}
          {% endif %}
          }
          </script>
{% endif %}
  
{% if template.name == "blog" %}

<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Blog",
  "name": "{{ blog.title | escape }}",
  "url": "{{ shop.url }}{{ blog.url }}",
  "description": "{{ blog.description | strip_html | escape }}",
  "blogPost": [
    {% for article in blog.articles %}
    {
      "@type": "BlogPosting",
      "headline": "{{ article.title | escape }}",
      "url": "{{ shop.url }}{{ article.url }}",
      "datePublished": "{{ article.published_at | date: '%Y-%m-%d' }}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
</script>
{% endif %}


