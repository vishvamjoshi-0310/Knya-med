    "breakpoint_faq_JceCrX": {
      "type": "breakpoint-faq",
      "blocks": {
        "faq_item_DjA4EH": {
          "type": "faq_item",
          "settings": {
            "question": "What is Breakpoint?",
            "answer": "Breakpoint is a 24/7, completely free mental health helpline for India’s medical community.  <br>\n\nThere is no cost to call, no limit on usage, and no requirement to justify why you’re reaching out. It’s designed for moments when things feel heavy and you need to talk to a trained professional immediately."
          }
        },
        "faq_item_gY6jPw": {
          "type": "faq_item",
          "disabled": true,
          "settings": {
            "question": "Help with how I'm feeling right now",
            "answer": "Breakpoint is a 24/7, completely free mental health helpline for India's medical community. There is no cost to call, no limit on usage, and no requirement to justify why you're reaching out. It's designed for moments when things feel heavy and you need to talk to a trained professional immediately."
          }
        },
        "faq_item_CLyAYW": {
          "type": "faq_item",
          "settings": {
            "question": "Is this therapy?",
            "answer": "No. Breakpoint is not therapy or long-term treatment.  <br>\n\nIt offers immediate emotional support to help you offload, ground yourself, and feel lighter in the moment without diagnosis or judgement. \n\nYou can call as many times as you need; every call is free."
          }
        },
        "faq_item_BRPWeg": {
          "type": "faq_item",
          "settings": {
            "question": "Who will I speak to?",
            "answer": "A trained mental health counsellor from Mpower, a mental health initiative of the Aditya Birla Education Trust.  <br>\n\nSomeone experienced in emotional de‑stress conversations, who listens first and supports you calmly and safely."
          }
        },
        "faq_item_MUQPpA": {
          "type": "faq_item",
          "settings": {
            "question": "Is my identity confidential?",
            "answer": "Yes. Completely.  <br>\n\nNo personal details are required. Nothing you share is recorded, reported, or shared with Knya, hospitals, employers, or anyone else."
          }
        },
        "faq_item_7wywgc": {
          "type": "faq_item",
          "settings": {
            "question": "Who is this helpline for?",
            "answer": "Exclusively for India’s medical community including interns, residents, doctors, nurses, paramedics, and allied healthcare professionals across specializations."
          }
        },
        "faq_item_7xnVVF": {
          "type": "faq_item",
          "settings": {
            "question": "What if I'm not sure my problem is \"serious enough\"?",
            "answer": "If it’s affecting you, it’s reason enough.  <br>\n\nYou don’t need a breakdown or a crisis to call. Sometimes, being heard at the right moment is enough to change how the day feels."
          }
        }
      },
