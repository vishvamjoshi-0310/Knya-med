                            <legend class="form__label abcgfiyuio font-heebo-regular">{{ option.name }}</legend>
                            <span class="color_name font-heebo-regular"></span>
                          </div>
                        {% else %}
                          <div class="varient_top_container">
                            <legend class="form__label  font-heebo-regular  ">
                              {{ option.name }}:
                              <span class="varient-title-SizeSwatchList-selected-value-{{ forloop.index }}"></span>
