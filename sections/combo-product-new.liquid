                            {% assign assetFolder = assetname | asset_url %}
                            <label
                              data-color="{{ value }}"
                              {% assign variant = product.variants | where: 'option1', value %}
                              {% for v in variant %}
                                variant-id="{{ v.id }}"
                              {% endfor %}
                              class="custom-swatch default-artt color_varient {{ value }}"
                              style="{%  if currentColorHashcode != '' %}background-color: {{ currentColorHashcode }} ;{% else %}background-color: {{ value | replace: ' ', '' | downcase }};{% endif %} padding: 0 0 !important;width: 25px; height: 25px;background-image:url('{{assetFolder}}'); background-color:{{ value }}"
                              for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
                              ><span class="swatch-titlee">{{ value | downcase }}</span>
                            </label>
                          {% else %}
                            <input
                              type="radio"
                              {% assign optionName = option.name | downcase | remove: ' ' %}
                              {% assign valueNew = value | replace: ' ', '' %}
                              {% if optionName == 'topsize' or optionName == 'size' or optionName == 'topstyle' %}
                                class="size_varient PointerNone removeDisabled"
                                {% if firstNameValueArrfinal contains valueNew -%}
                                  data-available="true"
                                {% elsif secondNameValueArrfinal contains valueNew %}
                                  data-available="true"
                                {% else %}
                                  data-available="false"
                                {% endif %}
                              {% elsif optionName == 'bottomsize' %}
                                class="size_varient size_varient1 PointerNone removeDisabled"
                                indexValue="{{ indexValue }}"
                                {% if indexValue == 3 %}
                                  {% assign thirdNameValueBottomStyle = thirdNameValueArr | replace: '# ', '' %}
                                  {% if thirdNameValueBottomStyle contains value %}
                                    data-available="true"
                                  {% else %}
                                    data-available="false"
                                  {% endif %}
                                {% elsif secondNameValueArrfinal contains value -%}
                                  data-available="true"
                                {% else %}
                                  data-available="false"
                                {% endif %}
                              {% elsif optionName == 'pack' %}
                                {% assign packValue = value | replace: ' ', '' %}
                                class="size_varient size_varient2 PointerNone removeDisabled"
                                {% if secondNameValueArrfinal contains packValue
                                  or thirdNameValueArrfinal contains packValue
                                -%}
                                  data-available="true"
                                {% else %}
                                  data-available="false"
                                {% endif %}
                              {% else %}
                                class="size_varient size_varient2 PointerNone removeDisabled"
                                {% if thirdNameValueArrfinal contains value -%}
                                  data-available="true"
                                {% else %}
                                  data-available="false"
                                {% endif %}
                              {% endif %}

                              id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
                              name="{{ option.name }}"
                              value="{{ value | escape }}"
                              form="{{ product_form_id }}"
                              onclick="stickyState()"
                            >
                            <label
                              class="custom-swatch disableBtn {% if optionName == 'pack' or optionName == 'size' or optionName == 'topstyle' %}design-pack-label{% endif %}"
                              for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
                            >
                              {{ value }}
                            </label>
                          {% endif %}
                        {%- endfor -%}
                      </fieldset>
                    {%- endfor -%}
                    <script type="application/json">
                      {{ product.variants | json }}
