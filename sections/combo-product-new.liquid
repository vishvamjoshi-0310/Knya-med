    "availability": "{% if product.selected_or_first_available_variant.available %}https://schema.org/InStock{% else %}https://schema.org/OutOfStock{% endif %}",
    "seller": {
      "@type": "Organization",
      "name": "{{ shop.name }}"
    }
  }
  {%- if product.metafields.reviews.rating.value != blank -%}
  ,
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "{{ product.metafields.reviews.rating.value.rating }}",
    "reviewCount": "{{ product.metafields.reviews.rating_count }}"
  }
  {%- endif -%}
   {%- if product.metafields.custom.fabric_details != blank or product.metafields.custom.product_catagory != blank or product.metafields.custom.pockets != blank or product.metafields.custom.colour != blank -%}
   ,
   "additionalProperty": [
     {%- assign property_count = 0 -%}
     {%- if product.metafields.custom.fabric_details != blank -%}
     {%- assign property_count = property_count | plus: 1 -%}
     {
       "@type": "PropertyValue",
       "name": "Fabric Composition",
       "value": "{{ product.metafields.custom.fabric_details | escape }}"
     }
     {%- endif -%}
     {%- if product.metafields.custom.product_catagory != blank -%}
     {%- if property_count > 0 -%},{%- endif -%}
     {%- assign property_count = property_count | plus: 1 -%}
     {
       "@type": "PropertyValue",
       "name": "Neck Style",
       "value": "{{ product.metafields.custom.product_catagory | escape }}"
     }
     {%- endif -%}
     {%- if product.metafields.custom.pockets != blank -%}
     {%- if property_count > 0 -%},{%- endif -%}
     {%- assign property_count = property_count | plus: 1 -%}
     {
       "@type": "PropertyValue",
       "name": "Number of Pockets",
       "value": "{{ product.metafields.custom.pockets | escape }}"
     }
     {%- endif -%}
     {%- if product.metafields.custom.colour != blank -%}
     {%- if property_count > 0 -%},{%- endif -%}
     {%- assign property_count = property_count | plus: 1 -%}
     {
       "@type": "PropertyValue",
       "name": "Color",
       "value": "{{ product.metafields.custom.colour | escape }}"
     }
     {%- endif -%}
   ]
   {%- endif -%}
}
