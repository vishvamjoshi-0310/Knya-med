<script>
  const box = document.querySelector('.product-desc');
  let isOpen = false;

  document.querySelector('.read-more-pro').addEventListener('click', () => {
    isOpen = !isOpen;
    isOpen ? box.classList.add('open') : box.classList.remove('open') 
    if(document.querySelector('.read-span').innerText === "Read More"){
      document.querySelector('.read-span').innerText = "Read Less"
    }else{
      document.querySelector('.read-span').innerText = "Read More"
    }
  });

  

    let mainHeight =  document.querySelector(".swiperMain .mainSliderImg-product img").clientHeight;
    document.querySelector(".swiperThumbnails").setAttribute("style",`height:${mainHeight}px!important`);

      
    /*
    * Function is called on DOMContentLoaded.
    */
    window.addEventListener('DOMContentLoaded', (event) => {

      var mySwiperMOdal = new Swiper(".mySwiperMOdal", {
        slidesPerView: 1,
        loop: true,
        zoom: true,
        navigation: {
          nextEl: ".swiper-button-nextModal",
          prevEl: ".swiper-button-prevModal",
        },
      });


      
      var swiperABC = new Swiper(".swiperThumbnails", {
            direction: 'vertical',
            spaceBetween: 10,
            slidesPerView: "auto",
            freeMode: false,
            loop: false,
          
          thumbs: {
                  swiper: swiperdef,
                },
          });

      
      var swiperdef = new Swiper(".swiperMain", {
            slidesPerView: 1,
            loop: false,
            thumbs: {
              swiper: swiperABC,
            },
            pagination: {
            el: ".swiper-pagination",
          },
            observer:true,
          });


        
        document.querySelectorAll('.swiperMain .swiper-slide .sliderr-img').forEach((each) => {
          each.addEventListener('click', function (r) {
            document.querySelector('.mobile-atc-pdp').style.display = 'none';
            document.querySelector('.product-media-modal').style.display = 'block';
            let indd = parseInt(r.target.closest('.swiper-slide').getAttribute('aria-label').split('/')[0]);
            document
              .querySelectorAll('.product-media-modal__content .swiper-slide .global-media-settings')
              .forEach((each2) => {
                let indt = each2.closest('.swiper-slide').getAttribute('aria-label').split('/')[0];
                if (indd == indt) {
                  mySwiperMOdal.slideTo(indt);
                }
              });
          });
        });

        document.querySelectorAll(".custom-swatch").forEach(e=>{
        
            e.addEventListener("click" , function(){
                document.querySelectorAll(".custom-swatch").forEach(each=>{
                    if(each.closest(".HorizontalList__Item").classList.contains("swatch-border")){
                    each.closest(".HorizontalList__Item").classList.remove("swatch-border")
                }
                })
                    e.closest(".HorizontalList__Item").classList.add("swatch-border")
                
            })
        })

        document.querySelectorAll(".SizeSwatch").forEach(e=>{
          e.addEventListener("click" , function(){
              document.querySelectorAll(".SizeSwatch").forEach(each=>{
                  if(each.closest(".HorizontalList__Item").classList.contains("swatch-border")){
                  each.closest(".HorizontalList__Item").classList.remove("swatch-border")
              }
              })
                  e.closest(".HorizontalList__Item").classList.add("swatch-border")
              
          })
        })

      {%- for option in product.options_with_values -%}
       {%- for value in option.values -%}
          {% if option.name == "Color" %}

            window.onload = function(){     
              document.querySelector(".custom-swatch").click();
            }

            document.querySelectorAll(".custom-swatch").forEach(r=>{
                r.addEventListener('click', function(){
                  document.querySelectorAll(".custom-swatch").forEach((e)=>{
                  e.classList.remove("active");
                  })
                  r.classList.add("active")
                  let swatchh = r.getAttribute("data-color")
                    sliderFunc(swatchh)
                })
            })


            function sliderFunc(t){
                document.querySelectorAll(".swiperMain .sliderr-img").forEach(v=>{
                      v.parentElement.parentElement.parentElement.parentElement.parentElement.style.display = 'none'
                    if(v.getAttribute("alt")==t){
                          v.parentElement.parentElement.parentElement.parentElement.parentElement.style.display = 'block'
                      
                    }
                })

                document.querySelectorAll(".swiperThumbnails img").forEach(v=>{
                    if(v.parentElement.parentElement.parentElement.classList.contains('swiper-slide')){
                      v.parentElement.parentElement.parentElement.style.display='none'
                      } else{
                      v.parentElement.parentElement.parentElement.parentElement.parentElement.style.display = 'none'
                    }
              
                  if(v.getAttribute("alt")==t){
                  if(v.parentElement.parentElement.parentElement.parentElement.classList.contains('swiper-slide')){
                      v.parentElement.parentElement.parentElement.parentElement.style.display='block'
                      }else{
                      v.parentElement.parentElement.parentElement.parentElement.parentElement.style.display = 'block'
                      }  
                  }
                });

                document.querySelectorAll(".mySwiperMOdal .sliderr-img").forEach(v=>{
                    v.parentElement.parentElement.parentElement.parentElement.parentElement.style.display = 'none'
                    if(v.getAttribute("alt")==t){
                          v.parentElement.parentElement.parentElement.parentElement.parentElement.style.display = 'block' 
                    }
                });

              swiperABC.update();
              swiperdef.update();
            }

          {% endif %}
        {% endfor %}
      {% endfor %}
    });

    //  var dd = document.querySelector(".swiperThumbnails .imagesss").offsetWidth;
    // document.querySelector(".swiperThumbnails deferred-media.videoosss").style.height = `${dd}px`;
 
    /*
    * Function is called on DOMContentLoaded (default method in dawn theme).
    */
    document.addEventListener('DOMContentLoaded', function() {
      function isIE() {
        const ua = window.navigator.userAgent;
        const msie = ua.indexOf('MSIE ');
        const trident = ua.indexOf('Trident/');

        return (msie > 0 || trident > 0);
      }

      if (!isIE()) return;
      const hiddenInput = document.querySelector('#{{ product_form_id }} input[name="id"]');
      const noScriptInputWrapper = document.createElement('div');
      const variantSwitcher = document.querySelector('variant-radios[data-section="{{ section.id }}"]') || document.querySelector('variant-selects[data-section="{{ section.id }}"]');
      noScriptInputWrapper.innerHTML = document.querySelector('.product-form__noscript-wrapper-{{ section.id }}').textContent;
      variantSwitcher.outerHTML = noScriptInputWrapper.outerHTML;

      document.querySelector('#Variants-{{ section.id }}').addEventListener('change', function(event) {
        hiddenInput.value = event.currentTarget.value;
      });
    });
</script>
<script>
