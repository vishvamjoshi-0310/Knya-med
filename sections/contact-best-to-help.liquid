{{ 'contact-best-to-help.css' | asset_url | stylesheet_tag }}
<style>
 .full-width-div1{margin-top:{{section.settings.margintophelp}}px}
   @media screen and (max-width: 640px){
    .full-width-div1{margin-top:{{section.settings.margintophelpmob}}px}
  }
</style>


<div class="full-width-div1 {% if section.settings.show_padding_help  %} page-padding-right page-padding-left{% endif %} {% if section.settings.show_padding_mob_help%}padding-right-mob padding-left-mob{% endif %}">
  <div class="image-full1">
                   <div class="help-div">
    <img class="desktop-image1"
srcset="{%- if section.settings.image_for_desk1.width >= 375 -%}{{ section.settings.image_for_desk1 | image_url: width: 375 }} 375w,{%- endif -%}
{%- if section.settings.image_for_desk1.width >= 550 -%}{{ section.settings.image_for_desk1 | image_url: width: 550 }} 550w,{%- endif -%}
{%- if section.settings.image_for_desk1.width >= 750 -%}{{ section.settings.image_for_desk1 | image_url: width: 750 }} 750w,{%- endif -%}
{%- if section.settings.image_for_desk1.width >= 1100 -%}{{ section.settings.image_for_desk1 | image_url: width: 1100 }} 1100w,{%- endif -%}
{%- if section.settings.image_for_desk1.width >= 1500 -%}{{ section.settings.image_for_desk1 | image_url: width: 1500 }} 1500w,{%- endif -%}
{%- if section.settings.image_for_desk1.width >= 1780 -%}{{ section.settings.image_for_desk1 | image_url: width: 1780 }} 1780w,{%- endif -%}
{%- if section.settings.image_for_desk1.width >= 2000 -%}{{ section.settings.image_for_desk1 | image_url: width: 2000 }} 2000w,{%- endif -%}
{%- if section.settings.image_for_desk1.width >= 3000 -%}{{ section.settings.image_for_desk1 | image_url: width: 3000 }} 3000w,{%- endif -%}
{%- if section.settings.image_for_desk1.width >= 3840 -%}{{ section.settings.image_for_desk1 | image_url: width: 3840 }} 3840w,{%- endif -%}
{{ section.settings.image_for_desk1 | image_url }} {{ section.settings.image_for_desk1.width }}w"
sizes="100vw"
src="{{ section.settings.image_for_desk1 |  img_url:"1600x" }}"
loading="lazy"
alt="{{ section.settings.image_for_desk1.alt | escape }}"
width="{{ section.settings.image_for_desk1.width }}"
height="{{ section.settings.image_for_desk1.height }}"
>
    <img class="mob-image1"
srcset="{%- if section.settings.image_for_mobile1.width >= 375 -%}{{ section.settings.image_for_mobile1 | image_url: width: 375 }} 375w,{%- endif -%}
{%- if section.settings.image_for_mobile1.width >= 550 -%}{{ section.settings.image_for_mobile1 | image_url: width: 550 }} 550w,{%- endif -%}
{%- if section.settings.image_for_mobile1.width >= 750 -%}{{ section.settings.image_for_mobile1 | image_url: width: 750 }} 750w,{%- endif -%}
{%- if section.settings.image_for_mobile1.width >= 1100 -%}{{ section.settings.image_for_mobile1 | image_url: width: 1100 }} 1100w,{%- endif -%}
{%- if section.settings.image_for_mobile1.width >= 1500 -%}{{ section.settings.image_for_mobile1 | image_url: width: 1500 }} 1500w,{%- endif -%}
{%- if section.settings.image_for_mobile1.width >= 1780 -%}{{ section.settings.image_for_mobile1 | image_url: width: 1780 }} 1780w,{%- endif -%}
{%- if section.settings.image_for_mobile1.width >= 2000 -%}{{ section.settings.image_for_mobile1 | image_url: width: 2000 }} 2000w,{%- endif -%}
{%- if section.settings.image_for_mobile1.width >= 3000 -%}{{ section.settings.image_for_mobile1 | image_url: width: 3000 }} 3000w,{%- endif -%}
{%- if section.settings.image_for_mobile1.width >= 3840 -%}{{ section.settings.image_for_mobile1 | image_url: width: 3840 }} 3840w,{%- endif -%}
{{ section.settings.image_for_mobile1 | image_url }} {{ section.settings.image_for_mobile1.width }}w"
sizes="100vw"
src="{{ section.settings.image_for_mobile1 |  img_url:"600x" }}"
loading="lazy"
alt="{{ section.settings.image_for_mobile1.alt | escape }}"
width="{{ section.settings.image_for_mobile1.width }}"
height="{{ section.settings.image_for_mobile1.height }}"
>
</div>
    <div class="full-width-text1 page-width font-heebo-medium"><h1 class="full-width-title-main">{{ section.settings.text-desk1 }}</h1></div>
  </div>
</div>



{% schema %}
  {
    "name": "Help Best section",
    "settings": [
      {
  "type": "checkbox",
  "id": "show_padding_help",
  "label": "padding for section",
  "default": true
},
    {
  "type": "checkbox",
  "id": "show_padding_mob_help",
  "label": "padding for section in mobile",
  "default": true
},
     {
  "type": "range",
  "id": "margintophelp",
  "min": 0,
  "max": 100,
  "step": 1,
  "label": "margin from top for entire section",
  "default": 0
},
    {
  "type": "range",
  "id": "margintophelpmob",
  "min": 0,
  "max": 100,
  "step": 1,
  "label": "margin from top for entire section",
  "default": 0
},
      {
  "type": "image_picker",
  "id": "image_for_desk1",
  "label": "Image for desktop"
},
      {
  "type": "text",
  "id": "text-desk1",
  "label": "Heading"
},
        {
  "type": "image_picker",
  "id": "image_for_mobile1",
  "label": "Image for mobile"
}
    ],
    "presets": [
    {
      "name": "helpinng best"
    }
  ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}