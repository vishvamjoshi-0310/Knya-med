    function handleAddPair(lineArray) {
    try {
      Shopify.queue = [];
      for (var i = 0; i < lineArray.length; i++) {
        Shopify.queue.push({
          line: lineArray[i].variantId,
          property: lineArray[i].lineItemProperties
        });
      }
      Shopify.moveAlong = function () {
        if (Shopify.queue.length) {
          var request = Shopify.queue.shift();
          let data = {
            'items': [{
              'id': request.line,
              'quantity': 1,
              'properties': request.property
            }]
          }
          fetch('/cart/add.js', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify(data)
          })
            .then(response => {
              if (response.ok) {
                return response.json();
              } else {
                throw new Error('Network response was not ok');
              }
            })
            .then(res => {
              // Handle the JSON response (res) here
              Shopify.moveAlong();
              if (document.querySelector(".product-form__buttons .loading-overlay__spinner") && document.querySelector(".product-form__buttons .loading-overlay__spinner").classList.contains("pdpLoader")) document.querySelector(".product-form__buttons .loading-overlay__spinner").classList.remove("pdpLoader");
        if (document.querySelector(".product-form__buttons .loading-overlay__spinner")) document.querySelector(".product-form__buttons .loading-overlay__spinner").style.display = "none";
            })
            .catch(error => {
              console.error('Error:', error);
            });
        } else {
   
           cartCardUpdate();
        document.querySelector('cart-drawer').classList.add('active')
        varientcontainer=[]
        }
      };
      Shopify.moveAlong();
    } catch (err) {
      console.log(err);
    }
  }
